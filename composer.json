{
    "name": "tonis/doctrine-orm-package",
    "authors": [
        {
            "name": "Kyle Spraggs",
            "email": "theman@spiffyjr.me"
        }
    ],
    "description": "Spiffy\\DoctrineORMPackage is a package for SpiffyFramework that integrates Doctrine ORM.",
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=5.5",
        "doctrine/orm": "^2.5",
        "tonis/doctrine-package": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0",
        "squizlabs/php_codesniffer": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Tonis\\DoctrineORMPackage\\": [
                "src/"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tonis\\DoctrineORMPackage\\": [
                "test/"
            ]
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "1.0-dev"
        }
    }
}
